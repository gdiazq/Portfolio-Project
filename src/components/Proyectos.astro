<script>
    const initCarousel = (carouselElement: HTMLElement) => {
        const state = {
            items: Array.from(carouselElement.querySelectorAll('[id^="carousel-item-"]')) as HTMLElement[],
            data: Array.from(carouselElement.querySelectorAll('[id^="text-carousel-"]')) as HTMLElement[],
            currentIndex: 0,
            prevButton: carouselElement.querySelector('#data-carousel-prev') as HTMLElement,
            nextButton: carouselElement.querySelector('#data-carousel-next') as HTMLElement
        };

        const updateCarousel = () => {
            state.items.forEach((item, index) => {
                item.style.display = index === state.currentIndex ? 'block' : 'none';
            });
        };

        const updateDataCarousel = () => {
            state.data.forEach((item, index) => {
                item.style.display = index === state.currentIndex ? 'block' : 'none';
            });
        };

        const showPreviousSlide = () => {
            state.currentIndex = (state.currentIndex > 0) ? state.currentIndex - 1 : state.items.length - 1;
            updateCarousel();
            updateDataCarousel();
        };

        const showNextSlide = () => {
            state.currentIndex = (state.currentIndex < state.items.length - 1) ? state.currentIndex + 1 : 0;
            updateCarousel();
            updateDataCarousel();
        };

        state.prevButton.addEventListener('click', showPreviousSlide);
        state.nextButton.addEventListener('click', showNextSlide);

        updateCarousel();
        updateDataCarousel();
    };

    if (typeof document !== 'undefined') {
        document.addEventListener('DOMContentLoaded', () => {
            const carouselElement = document.getElementById('carousel-example');
            if (carouselElement) {
                initCarousel(carouselElement);
            }
        });
    };
</script>